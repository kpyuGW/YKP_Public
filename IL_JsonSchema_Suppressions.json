{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://raw.githubusercontent.com/kpyuGW/YKP_Public/main/IL_JsonSchema_CommonObj.json",
	"title": "GXAI-IL-Suppressions", 
	"description": "Schema Definition of Suppressions JSON from GXAI", 
	"type" : "object", 
	"properties" : {
		"rSuppressions" : {
			"type" : "array", 
			"items" : {
				"type" : "object", 
				"properties" : {
					"suppressionRules" : {
						"type" : "array", 
						"items" : { "$ref" : "#/$defs/objRuleContent" }
					}
				}, 
				"required" : [ "suppressionRules" ]
			}
		}		
	}, 
	"required" : [ "rSuppressions" ]	
}
